! font settings --------------------------------------------------------------

Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
!Xft.dpi: 96
!Xft.rgba: rgb
!Xft.lcdfilter: lcddefault

! urxvt ----------------------------------
urxvt*font: xft:terminus:size=7
urxvt*boldFont: xft:terminus:bold:size=7


! colors!!
urxvt*background: [70]#fef9ec
urxvt*borderColor: [70]#fef9ec
urxvt*foreground: #6d727e
urxvt*cursorColor: #007693
!urxvt*cursorColor2:
urxvt*colorBD: #3e424d
urxvt*colorIT: #3e424d
urxvt*colorUL: #3e424d
!urxvt*underlineColor:
!urxvt*highlightColor:
!urxvt*highlightTextColor:
!urxvt*pointerColor:
!black
*color0:    #c5c1b4
*color8:    #a1a6b2
!!red
*color1:    #e75c58
*color9:    #b22b31
!!green
*color2:    #00a250
*color10:   #007427
!!yellow
*color3:    #a08a00
*color11:   #715f00
!!blue
*color4:    #208ffb
*color12:   #0065ca
!!magenta
*color5:    #d160c4
*color13:   #a03196
!!cyan
*color6:    #00a4c1
*color14:   #007693
!!white
*color7:    #3e424d
*color15:   #3e424d

!extensions
!urxvt*perl-ext-common: default,matcher,tabbed
!urxvt*matcher.button: 1


!appearance
urxvt*scrollBar: false
urxvt*cursorBlink: true
urxvt*depth: 32 !transparency
!urxvt*iconFile: /path/to/icon.svg
!urxvt*fading: 70
!urxvt*fadeColor: [30]#ff0000
!urxvt*transparent:true
!urxvt*blurRadius: 9

!other settings
urxvt*saveLines: 12000
urxvt*scrollStyle: plain
urxvt*imLocale: en_US.UTF-8
